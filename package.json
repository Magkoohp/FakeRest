{
  "name": "fakerest",
  "version": "3.5.0",
  "repository": "https://github.com/marmelab/FakeRest",
  "description": "Patch XMLHttpRequest to fake a REST server based on JSON data. ",
  "scripts": {
    "build": "vite build && vite build -c vite.config.min.ts",
    "format": "biome format --write src",
    "lint": "biome lint --apply src",
    "test": "vitest",
    "prepare": "husky"
  },
  "main": "dist/fakerest.min.umd.js",
  "module": "./dist/fakerest.min.mjs",
  "exports": {
    ".": {
      "import": "./dist/fakerest.min.mjs",
      "require": "./dist/fakerest.umd.js"
    }
  },
  "author": "Fran√ßois Zaninotto <fzaninotto@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.7.0",
    "fetch-mock": "^9.11.0",
    "happy-dom": "^14.7.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "sinon": "~1.14.1",
    "vite": "^5.2.9",
    "vitest": "^1.5.0"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  },
  "browserslist": "> 0.25%, not dead",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "biome lint --apply",
      "biome format --write"
    ]
  }
}
